{
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": "*.{js,jsx}",
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.less",
      "options": {
        "parser": "less"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css"
      }
    },
    {
      "files": "*.{scss,sass}",
      "options": {
        "parser": "scss"
      }
    },
    {
      "files": "*.{yaml,yml}",
      "options": {
        "parser": "yaml"
      }
    },
    {
      "files": "*.{graphql,graphqls}",
      "options": {
        "parser": "graphql"
      }
    },
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    },
    {
      "files": "*.{json,jsonschema,graphqlconfig}",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.{html,ejs,svg,xml}",
      "options": {
        "singleQuote": false,
        "parser": "html"
      }
    },
    {
      "files": "*.mdx",
      "options": {
        "singleQuote": false,
        "parser": "mdx"
      }
    },
    {
      "files": [
        "*.md"
      ],
      "options": {
        "singleQuote": false,
        "parser": "markdown"
      }
    }
  ],
  "printWidth": 160,
  "quoteProps": "consistent",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
