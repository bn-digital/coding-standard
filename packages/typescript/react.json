{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true
  }
}
