{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true
  }
}
