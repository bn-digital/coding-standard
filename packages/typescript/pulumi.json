{
  "extends": "@tsconfig/node18",
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["ESNext"],
    "module": "commonjs",
    "noEmit": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "outDir": "bin",
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2016"
  }
}
